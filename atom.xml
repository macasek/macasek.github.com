---
layout: nil
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
 
 <title>{{ site.params.author }}</title>
 <link href="http://{{ site.params.github }}.github.com/atom.xml" rel="self"/>
 <updated>{{ site.time | date_to_xmlschema }}</updated>
 <id>http://{{ site.params.github }}.github.com/</id>
 <author>
   <name>{{ site.params.author }}</name>
   <email>{{ site.params.email }}</email>
 </author>
 
 {% for post in site.posts %}
 <entry>
   <title>{{ post.title }}</title>
   <link href="http://{{ site.params.github }}.github.com/{{ post.url }}"/>
   <updated>{{ post.date | date_to_xmlschema }}</updated>
   <id>http://schacon.github.com/{{ post.id }}</id>
   <content type="html">{{ post.content | xml_escape }}</content>
 </entry>
 {% endfor %}
 
</feed>